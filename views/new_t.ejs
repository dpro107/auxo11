            fetchData.forEach(function(data){
            %>
           
            <tr>
              
                <td><%=i; %></td>
                <td><%=data.customer_id %></td>
                <td><%=data.customer_name %></td>
                <td><%=data.phone_number %></td>
    
                <td><%=data.email_address %></td>
                <td><%=data.counsellor %></td>
                <td><%=data.batch_number %></td>
