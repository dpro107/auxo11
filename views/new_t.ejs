                <td><%=data.refund_process_date %></td>
                <td><%=data.refund_date %></td>
                <td><%=data.running_batch_start_date %></td>
                <td><%=data.due_notdue %></td>
                <td><%=data.due %></td>
                <td><%=data.min_50_received%></td>
                <td><a href="/crud/edit/<%=data.id%>" >Edit for sales</a></td>
    
            </tr>
            <%  i++; }) %>
            <% } else{ %>
                <tr>
                    <td colspan="7">No Data Found</td>
                </tr>
            <% } %>
        </table>
        </div>
    
    
